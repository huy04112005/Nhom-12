<odoo>
  <record id="view_meeting_room_form" model="ir.ui.view">
    <field name="name">meeting.room.form</field>
    <field name="model">meeting.room</field>
    <field name="arch" type="xml">
      <form>
        <sheet>
          <group>
            <field name="name"/>
            <field name="location"/>
            <field name="capacity"/>
            <field name="status"/>
            <field name="description"/>
          </group>
        </sheet>
      </form>
    </field>
  </record>
</odoo>
